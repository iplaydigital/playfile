  `vdo_id` int(11) NOT NULL AUTO_INCREMENT,
  `category_id` int(11) NOT NULL DEFAULT '0',
  `vdo_title` varchar(255) DEFAULT NULL,
  `vdo_detail` longtext NOT NULL,
  `vdo_value` varchar(255) DEFAULT NULL,
  `vdo_status` enum('approved','pending','discard') NOT NULL DEFAULT 'approved',
