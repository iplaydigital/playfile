  `menu_id` int(11) NOT NULL AUTO_INCREMENT,
  `menu_subid` int(11) NOT NULL DEFAULT '0',
  `menu_class` varchar(255) DEFAULT NULL,
  `menu_title_th` varchar(255) NOT NULL DEFAULT 'title_th',
  `menu_title_en` varchar(255) NOT NULL DEFAULT 'title_en',
  `menu_link` text,
  `menu_order` int(11) NOT NULL DEFAULT '0',
  `menu_status` enum('approved','pending','discard') NOT NULL DEFAULT 'approved',
  `menu_createdtime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `menu_createdip` varchar(15) NOT NULL DEFAULT '000.000.000.000',
  `menu_updatedtime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `menu_updatedip` varchar(15) NOT NULL DEFAULT '000.000.000.000',
  PRIMARY KEY (`menu_id`),
  KEY `submenu` (`menu_subid`)
) ENGINE=MyISAM AUTO_INCREMENT=9 DEFAULT CHARSET=utf8;
/*!40101 SET character_set_client = @saved_cs_client */;

