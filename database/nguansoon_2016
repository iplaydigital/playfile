  `vdo_createdip` varchar(15) NOT NULL DEFAULT '000.000.000.000',
  `vdo_updatedtime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `vdo_updatedip` varchar(15) NOT NULL DEFAULT '000.000.000.000',
  PRIMARY KEY (`vdo_id`),
  KEY `category` (`category_id`)
) ENGINE=MyISAM AUTO_INCREMENT=3 DEFAULT CHARSET=utf8;
