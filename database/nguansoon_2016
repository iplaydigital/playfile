--
-- Table structure for table `news`
--

DROP TABLE IF EXISTS `news`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `news` (
  `news_id` int(11) NOT NULL AUTO_INCREMENT,
  `news_tbm1` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `news_tbm2` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `news_tbm3` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `news_tbm4` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `news_tbm5` varchar(255) COLLATE utf8_unicode_ci NOT NULL,
  `news_title_en` varchar(255) COLLATE utf8_unicode_ci NOT NULL DEFAULT '-',
  `news_title_th` varchar(255) COLLATE utf8_unicode_ci NOT NULL DEFAULT '-',
  `news_sdesc_en` text COLLATE utf8_unicode_ci NOT NULL,
  `news_sdesc_th` text COLLATE utf8_unicode_ci NOT NULL,
  `news_desc_en` longtext COLLATE utf8_unicode_ci NOT NULL,
  `news_desc_th` longtext COLLATE utf8_unicode_ci NOT NULL,
  `news_status` enum('approved','pending','discard') COLLATE utf8_unicode_ci NOT NULL DEFAULT 'approved',
  `news_createdtime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `news_createdip` varchar(15) COLLATE utf8_unicode_ci NOT NULL DEFAULT '000.000.000.000',
  `news_updatedtime` datetime NOT NULL DEFAULT '0000-00-00 00:00:00',
  `news_updatedip` varchar(15) COLLATE utf8_unicode_ci NOT NULL DEFAULT '000.000.000.000',
  PRIMARY KEY (`news_id`)
) ENGINE=MyISAM AUTO_INCREMENT=16 DEFAULT CHARSET=utf8 COLLATE=utf8_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

